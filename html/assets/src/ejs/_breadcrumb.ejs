<script type="application/ld+json">
{
	"@context": "http://schema.org",
	"@type": "BreadcrumbList",
	"itemListElement": [
		{
			"@type": "ListItem",
			"position": 1,
			"item": {
				"@id": "<%= site_url %>",
				"name": "TOP"
			}
		},
		<% if(grand_slug){ %>
		{
			"@type": "ListItem",
			"position": 2,
			"item": {
				"@id": "<%= site_url %>/<%= grand_slug %>/",
				"name": "<%= grand_name %>"
			}
		},
		{
			"@type": "ListItem",
			"position": 3,
			"item": {
				"@id": "<%= site_url %>/<%= grand_slug %>/<%= parent_slug %>/",
				"name": "<%= parent_name %>"
			}
		},
		{
			"@type": "ListItem",
			"position": 4,
			"item": {
				"@id": "<%= site_url %>/<%= grand_slug %>/<%= parent_slug %>/<%= page_slug %>/",
				"name": "<%= page_name %>"
			}
		}
		<% }else if(parent_slug){ %>
		{
			"@type": "ListItem",
			"position": 2,
			"item": {
				"@id": "<%= site_url %>/<%= parent_slug %>/",
				"name": "<%= parent_name %>"
			}
		},
		{
			"@type": "ListItem",
			"position": 3,
			"item": {
				"@id": "<%= site_url %>/<%= parent_slug %>/<%= page_slug %>/",
				"name": "<%= page_name %>"
			}
		}
		<% }else{ %>
		{
			"@type": "ListItem",
			"position": 2,
			"item": {
				"@id": "<%= site_url %>/<%= page_slug %>/",
				"name": "<%= page_name %>"
			}
		}
		<% } %>
	]
}
</script>